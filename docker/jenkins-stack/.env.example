# Jenkins & CI/CD Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to git!

# Discord Webhook (replace with your Discord channel webhook URL)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# SonarQube Configuration
SONARQUBE_DB_PASSWORD=sonarpass
SONARQUBE_TOKEN=# Will be generated after first login to SonarQube

# AWS Credentials (use your f1-admin user credentials)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# GitHub Personal Access Token
GITHUB_TOKEN=your_github_pat_here

# Jenkins Admin
JENKINS_ADMIN_USER=admin
JENKINS_ADMIN_PASSWORD=choose_a_secure_password
